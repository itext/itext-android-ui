<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="file_uri" format="string" />
    <attr name="file_name" format="string" />
    <attr name="primary_color" format="string" />
    <attr name="secondary_color" format="string" />

    <declare-styleable name="PdfFragment">
        <attr name="file_uri" />
        <attr name="file_name" />
        <attr name="display_file_name" format="boolean" />
        <attr name="page_spacing" format="integer" />
        <attr name="enable_thumbnail_navigation_view" format="boolean" />
        <attr name="enable_split_view" format="boolean" />
        <attr name="enable_annotation_rendering" format="boolean" />
        <attr name="enable_double_tap_zoom" format="boolean" />
        <attr name="show_scroll_indicator" format="boolean" />
        <attr name="show_scroll_indicator_page_number" format="boolean" />
        <attr name="primary_color" />
        <attr name="secondary_color" />
        <attr name="background_color" format="string" />
    </declare-styleable>
    <declare-styleable name="SplitDocumentFragment">
        <attr name="file_uri" />
        <attr name="file_name" />
        <attr name="primary_color" />
        <attr name="secondary_color" />
    </declare-styleable>
</resources>